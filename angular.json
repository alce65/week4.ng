{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "demo1": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/demo1",
      "sourceRoot": "projects/demo1/src",
      "prefix": "isdi",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/demo1",
            "index": "projects/demo1/src/index.html",
            "browser": "projects/demo1/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/demo1/tsconfig.app.json",
            "assets": [
              "projects/demo1/src/favicon.ico",
              "projects/demo1/src/assets"
            ],
            "styles": ["projects/demo1/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "demo1:build:production"
            },
            "development": {
              "buildTarget": "demo1:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "demo1:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/demo1/tsconfig.spec.json",
            "assets": [
              "projects/demo1/src/favicon.ico",
              "projects/demo1/src/assets"
            ],
            "styles": ["projects/demo1/src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/demo1/**/*.ts",
              "projects/demo1/**/*.html"
            ]
          }
        }
      }
    },
    "challenge1": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/challenge1",
      "sourceRoot": "projects/challenge1/src",
      "prefix": "isdi",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/challenge1",
            "index": "projects/challenge1/src/index.html",
            "browser": "projects/challenge1/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/challenge1/tsconfig.app.json",
            "assets": [
              "projects/challenge1/src/favicon.png",
              "projects/challenge1/src/assets"
            ],
            "styles": ["projects/challenge1/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "challenge1:build:production"
            },
            "development": {
              "buildTarget": "challenge1:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "challenge1:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/challenge1/tsconfig.spec.json",
            "assets": [
              "projects/challenge1/src/favicon.ico",
              "projects/challenge1/src/assets"
            ],
            "styles": ["projects/challenge1/src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/challenge1/**/*.ts",
              "projects/challenge1/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": ["@angular-eslint/schematics"]
  }
}
